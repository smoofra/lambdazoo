{
    "compilerOptions": {
        "outDir": "./lib",
        "moduleResolution": "node",
        "target": "ES6",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
    }
}