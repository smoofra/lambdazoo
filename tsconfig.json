{
    "compilerOptions": {
        "outDir": "./dist",
        "moduleResolution": "node",
        "target": "ES2022",
        "esModuleInterop": true,
        "strict":  true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
    }
}